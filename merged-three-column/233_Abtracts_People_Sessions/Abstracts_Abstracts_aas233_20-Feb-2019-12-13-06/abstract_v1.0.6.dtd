<!ELEMENT SCORE_COLUMN_DESC (#PCDATA)>
<!ELEMENT SCORE_COLUMN_ROLE (#PCDATA)>
<!ELEMENT SCORE_NAME (#PCDATA)>
<!ELEMENT SCORE_VALUE (#PCDATA)>

<!ELEMENT RECOMMENDATION (RECOMMENDATION_NAME, RECOMMENDATION_DESC, RECOMMENDATION_ROLE)>
