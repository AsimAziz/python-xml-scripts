<?xml version="1.0" encoding="UTF-8"?>
<!ELEMENT AC_DATA (SITE_URL, ABSTRACTS?)>
<!ELEMENT SITE_URL (#PCDATA)>
<!ELEMENT ABSTRACTS (ABSTRACT+)>
<!ELEMENT ABSTRACT (STATUS, DECISION_STATUS, DECISION_HISTORY, TITLE, PRESENTATION_TYPE, SUB_PRESENTATION_TYPE?, SUBMISSION_ROLE?, ABSTRACT_CATEGORY?, TABLES?, IMAGES?, CONTACT_AUTHOR?, AUTHORS?, BODY?, ABSTRACT_DISCLOSURES?, ABSTRACT_DETAILS?, KEYWORDS?, AWARDS?, SPONSOR?, PAYMENTS?, SUPPLEMENTAL_DATA*, REVIEWS?, DECISIONS?, FINAL_IDS?, SYSTEM_TAGS?, CUSTOM_FIELD_1?)>
<!ATTLIST ABSTRACT
  id CDATA #REQUIRED
>
<!ELEMENT STATUS (#PCDATA)>
<!ELEMENT DECISION_STATUS (#PCDATA)>
<!ELEMENT DECISION_HISTORY (#PCDATA)>
<!ELEMENT TITLE (#PCDATA)>
<!ELEMENT PRESENTATION_TYPE (#PCDATA)>
<!ELEMENT SUB_PRESENTATION_TYPE (#PCDATA)>

<!ELEMENT SUBMISSION_ROLE (ROLE_NAME, ROLE_ID)>
