<!ELEMENT CONFLICT_FL (#PCDATA)>
<!ELEMENT WRONG_CAT_FL (#PCDATA)>
<!ELEMENT COMMENT (#PCDATA)>
<!ELEMENT USER_DEFINED_1 (#PCDATA)>
<!ELEMENT USER_DEFINED_2 (#PCDATA)>
<!ELEMENT USER_DEFINED_3 (#PCDATA)>
<!ELEMENT USER_DEFINED_4 (#PCDATA)>
