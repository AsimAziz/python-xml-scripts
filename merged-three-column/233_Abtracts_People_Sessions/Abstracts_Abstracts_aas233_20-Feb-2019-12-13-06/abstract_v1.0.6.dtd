  id CDATA #REQUIRED
>
<!ELEMENT DETAIL ((DETAIL_NAME, RESPONSE_NAME, RESPONSE_VALUE, FILE_LOCATION*))>
<!ATTLIST DETAIL
  id CDATA #REQUIRED
>
<!ELEMENT DETAIL_NAME (#PCDATA)>
<!ELEMENT RESPONSE_NAME (#PCDATA)>
<!ELEMENT RESPONSE_VALUE (#PCDATA)>

<!ELEMENT KEYWORDS (KEYWORD+)>
<!ELEMENT KEYWORD (#PCDATA)>

<!ELEMENT AWARDS (AWARD+)>
<!ELEMENT AWARD (NAME, DESC)>
<!ATTLIST AWARD
  id CDATA #REQUIRED
>
<!ELEMENT DESC (#PCDATA)>

<!ELEMENT SPONSOR (FIRSTNAME, LASTNAME, EMAIL_ADDRESS, PHONE, DEPARTMENT, INSTITUTION)>
<!ATTLIST SPONSOR
  sponsor_id CDATA #REQUIRED
