<!ELEMENT AFF_NAME (#PCDATA)>
<!ELEMENT AFF_TYPE (#PCDATA)>
<!ELEMENT AFF_INFO (#PCDATA)>
<!ELEMENT AFF_SPEAKER (#PCDATA)>

<!ELEMENT DESIGNATIONS (DESIG+)>
<!ELEMENT DESIG (NAME, DATA)>
<!ELEMENT DATA (#PCDATA)>

<!ELEMENT AFFILIATIONS (AFFL+)>
<!ATTLIST AFFL
  author_order CDATA #REQUIRED
  number CDATA #REQUIRED
>
<!ELEMENT AFFL (DEPT, INST, CITY, STATE, COUNTRY)>
<!ELEMENT DEPT (#PCDATA)>
<!ELEMENT INST (#PCDATA)>

<!ELEMENT BODY (SECTION+)>
<!ELEMENT SECTION (SECTION_NAME, TEXT)>
<!ATTLIST SECTION
  part_of_body CDATA #REQUIRED
>
<!ELEMENT SECTION_NAME (#PCDATA)>
<!ATTLIST SECTION_NAME
  bold (true | false) #REQUIRED
  italic (true | false) #REQUIRED
  underline (true | false) #REQUIRED
  name_appears (true | false) #REQUIRED
>
<!ELEMENT TEXT (#PCDATA)>

<!ELEMENT ABSTRACT_DISCLOSURES (ABS_DISC+)>
<!ELEMENT ABS_DISC (NAME, QUESTION, ANSWER, DISCLOSURE_AFFL*)>
