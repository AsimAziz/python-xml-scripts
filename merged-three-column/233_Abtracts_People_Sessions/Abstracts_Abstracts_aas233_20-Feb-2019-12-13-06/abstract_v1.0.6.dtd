<!ELEMENT PAYFNAME (#PCDATA)>
<!ELEMENT PAYLNAME (#PCDATA)>
<!ELEMENT PAYAMOUNT (#PCDATA)>
<!ELEMENT COMPANY_NAME (#PCDATA)>
<!ELEMENT ADDRESS1 (#PCDATA)>
<!ELEMENT ZIPCODE (#PCDATA)>
<!ELEMENT PNREF (#PCDATA)>
<!ELEMENT AUTH_CODE (#PCDATA)>
<!ELEMENT RESPONSE_MESSAGE (#PCDATA)>
<!ELEMENT CC_TYPE (#PCDATA)>
<!ELEMENT PAYMENT_STATUS (#PCDATA)>
<!ELEMENT PAYMENT_DATE (DAY_NAME, YEAR, MONTH, DAY)>
<!ELEMENT DAY_NAME (#PCDATA)>
<!ELEMENT YEAR (#PCDATA)>
<!ELEMENT MONTH (#PCDATA)>
<!ELEMENT DAY (#PCDATA)>

<!ELEMENT SUPPLEMENTAL_DATA (FILE_LOCATION, NAME, DESC, DISPLAY_NAME)>
<!ATTLIST SUPPLEMENTAL_DATA
  supplemental_data_id CDATA #REQUIRED
  supplemental_data_type CDATA #REQUIRED
  order CDATA #REQUIRED
>
